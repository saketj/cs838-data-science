{
    "dimensions": [
        {
         "name":"area",
         "levels": [
                {
                    "name":"zipcode",
                    "label":"ZipCode"
                },
                {
                    "name":"neighborhood",
                    "label":"Neighborhood"
                },
                {
                    "name":"borough",
                    "label":"borough"
                }
            ],
          "hierarchies": [
                {
