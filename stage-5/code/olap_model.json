                    {
                        "name": "real_estate_value_avg",
                        "function": "avg",
                        "measure": "median_real_estate_value"
                    },
                    {
                        "name": "cost_of_living_avg",
                        "function": "avg",
                        "measure": "cost_of_living"
                    },
                    {
                        "name": "population_avg",
                        "function": "avg",
                        "measure": "population"
                    },
                    {
                        "name": "num_restaurants",
                        "function": "count"
                    }
                ],
            "mappings": {
                          "area.zipcode": "zipcode",
